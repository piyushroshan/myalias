{
    "manifest_version": 2,
    "name": "MyAlias",
    "version": "1.3",

    "applications": {
        "gecko": {
            "id": "myalias@zecuguardian.com",
            "strict_min_version": "42.0"
        }
    },

    "description": "Associate short strings, aliases, with URLs",

    "icons": {
        "48": "icon_48.png",
        "64": "icon_64.png",
        "96": "icon_96.png"
    },

    "permissions": [
        "storage",
        "tabs"
    ],

    "omnibox": {
        "keyword": "u"
    },

    "background": {
        "scripts": ["background.js"]
    },

    "options_ui": {
        "page": "options.html"
    }
}
